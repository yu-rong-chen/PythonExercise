    "python.linting.pycodestyleEnabled": false,
    "python.linting.pycodestyleArgs": [
        "--max-line-length=99",
    ],
    "python.linting.pylintArgs": [
        "--disable=import-error, missing-module-docstring, missing-class-docstring, missing-function-docstring"
